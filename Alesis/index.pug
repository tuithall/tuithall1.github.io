                        div.key-b__back.face 
                        div.key-b__right.face 
                        div.key-b__left.face 
                        div.key-b__top.face 
                        div.key-b__bottom.face
                    - n++
            div.controls
                div.equ
                    - var i = 1
                    while i < 7
                        div.equ__item
                            div.circle.flex
                                div.circle-a  
                                    - var n = 0
                                    while n < 36
                                        div.circle-a__side
                                        - n++
                                    div.circle-a__bottom
